{
  "name": "e-tekana",
  "version": "1.0.1",
  "description": "e-wallet back-end service",
  "main": "index.js",
  "scripts": {
    "dev:db-seed-test-user": "npx sequelize db:seed --seed testUsers.js",
    "dev:db-setup": "npm run migrate:drop && npm run migrate:up && npm run dev:db-seed-test-user",
    "dev:server": "export NODE_ENV=development && nodemon --exec babel-node src/server.js",
    "migrate:up": "npx sequelize-cli db:migrate",
    "seed:up": "npx sequelize-cli db:seed:all",
    "migrate:drop": "npx sequelize-cli db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxahirwe/e-tekana.git.git"
  },
  "author": "Max Ahirwe",
